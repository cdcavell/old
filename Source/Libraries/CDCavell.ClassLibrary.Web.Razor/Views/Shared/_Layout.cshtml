    <noscript>
        <style nonce="@ViewBag.StyleNonce">
            #processing {
                display: none;
            }
            .page-container {
                display: none;
            }
        </style>
    </noscript>
</head>
<body>

    <div class="page-container">
        <partial name="_Nav" />

        <div class="background-image"></div>
        <div class="background-gradient"></div>
        <div class="container-fluid">
            <div class="body-container mb-5" id="container">
                @RenderBody()
            </div>
        </div>

        <partial name="_Foot" />
    </div>
    <div id="processing" class="modal fade" tabindex="-1" role="dialog" aria-label="processingLabel" aria-hidden="true" data-backdrop="static">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content border-0 shadow bg-processing">
                <div class="modal-body p-0">
                    <h3 class="text-center pt-2">Processing, please wait ...</h3>
                </div>
            </div>
        </div>
    </div>
    <partial name="_CookieConsentPartial" />
    <noscript>
        <div class="background-image"></div>
        <div class="background-gradient"></div>
        <div class="noscriptmsg" id="noscriptmsg">
            <div class="alert alert-danger" role="alert">
                JavaScript must be enabled in order for you to use cdcavell.name.
                However, it seems JavaScript is either disabled or not supported by your browser.
            </div>
        </div>
    </noscript>

    <script type="text/javascript" src="_content/CDCavell.ClassLibrary.Web.Razor/lib/jquery/jQuery-3.5.1-min.js" asp-append-version="true" nonce="@ViewBag.ScriptNonce"></script>
