    <script type="text/javascript" src="_content/CDCavell.ClassLibrary.Web.Razor/js/site.min.js" asp-append-version="true" nonce="@ViewBag.ScriptNonce"></script>

    <partial name="_CookieConsentPartialScript" />

    @RenderSection("scripts", required: false)

</body>
</html>
